<!DOCTYPE html>
<html>
	<head>
	</head>

	<body>
		<%= form_for :post, url: posts_path do |f| %>
			<p>
				<%= f.label :title %></br>
				<%= f.text_field :title %> 
			</p>
			<p>
				<%= f.label :text %> </br>
				<%= f.text_area :text %> 
			</p>
			<p>
				<%= f.label :photo %></br>
				<%= f.file_field :photo %>
			</p>
			<p>
				<%= f.submit %>
			</p>
		<% end %>
	</body>
</html>
